import scipy.integrate as integrate
import scipy.special as special
from scipy.integrate import quad
from numpy import sqrt, sin, cos, pi, e

def f(x):
    B = 0.000000000053333
    C = 1 #C is not really 1 but it is the constant before 
    return C * x*(e**(x)*(2-x)**(2)-32)/(e**(B*x))


print(f(0)) #to test if it works

